# git
abbr -g "g"="git"
abbr -g "gcm"="git commit -m"
abbr -g "gco"="git checkout"
abbr -g "gdc"="git diff --cached"
abbr -g "gdi"="git diff"
abbr -g "gdt"="git difftool -y"
abbr -g "gp"="git push"
abbr -g "gpf"="git push -f"
abbr -g "gft"="git fetch"
abbr -g "gsm"="git submodule"
abbr -g "gsu"="git submodule update --init --recursive"
abbr -g "gst"="git status"
abbr -g "git br"="git branch"
abbr -g "git ci"="git commit"
abbr -g "git cm"="git commit -m"
abbr -g "git co"="git checkout"
abbr -g "git dc"="git diff --cached"
abbr -g "git d"="git diff"
abbr -g "git di"="git diff"
abbr -g "git dt"="git difftool -y"
abbr -g "git sm"="git submodule"
abbr -g "git st"="git status"
abbr -g "git submodule u"="git submodule update --init --recursive"
abbr -g "git wt"="git worktree"

# cargo
abbr -g "c"="cargo"
abbr -g "cargo a"="cargo add"
abbr -g "cargo br"="cargo build --release"
abbr -g "cargo rr"="cargo run --release"
abbr -g "cargo tr"="cargo test --release"
abbr -g "cargo c"="cargo clean"
abbr -g "cargo cr"="cargo clean --release"
abbr -g "cargo ch"="cargo check"
